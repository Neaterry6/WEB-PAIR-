
{
  "version": 2,
  "builds": [
    {
      "src": "main.html",
      "use": "@vercel/static"
    },
    {
      "src": "pair.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/main.html"
    },
    {
      "src": "/pair",
      "dest": "/pair.html"
    }
  ]
}
